@model AllImages

@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <title>Nbrand.it</title>
        <link rel="stylesheet" href="../../Assets/style.css">
</head>
<body>
     <div class="menu">
        <div class="menu-title">Nbrand.it</div>
        <div class="menu-items">
            <div class="menu-items-item">About</div> 
            <div class="menu-items-item">What we do</div> 
            <div class="menu-items-item">Help</div>         
        </div>
        <div class="menu-profile">14 credits</div>
    </div>
     @for(int i = 0; i < Model.Images.Count()-1; i++)
    {<div><img src= "@Url.Content("~/Uploads/"+Model.Images[i])" alt="Image" /></div>}
        <br>
     @foreach(string str in Model.vals){
        <div>@str</div>}
     <br><br>
     <div><img src= "@Url.Content("~/Uploads/"+Model.Images[Model.Images.Count()-1])" alt="Image" /></div>
</body>
</html>